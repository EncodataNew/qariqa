<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="ir_cron_delete_unnecessary_logs" model="ir.cron">
        <field name="name">Delete Unnecessary Wallbox Logs</field>
        <field name="model_id" ref="model_wallbox_log"/>
        <field name="state">code</field>
