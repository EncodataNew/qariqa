<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="seq_wallbox_order" model="ir.sequence">
        <field name="name">Wallbox Order Sequence</field>
        <field name="code">wallbox.order</field>
        <field name="prefix">WO/</field>
        <field name="padding">4</field>
    </record>

    <record id="seq_wallbox_installation" model="ir.sequence">
        <field name="name">Wallbox Installation Sequence</field>
        <field name="code">wallbox.installation</field>
        <field name="prefix">WI/</field>
        <field name="padding">4</field>
    </record>

    <record id="seq_charging_station" model="ir.sequence">
        <field name="name">Charging Station Sequence</field>
        <field name="code">charging.station</field>
        <field name="prefix">CS/</field>
        <field name="padding">4</field>
    </record>

