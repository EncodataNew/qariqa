# Backend API Configuration
# Copy this file to .env and update with your actual values

# URL of your backend API (without trailing slash)
# Example: https://your-api.example.com or http://localhost:8069
VITE_API_BASE_URL=http://localhost:8069

# API request timeout in milliseconds (30 seconds)
VITE_API_TIMEOUT=30000

# Token refresh interval in milliseconds (5 minutes)
VITE_TOKEN_REFRESH_INTERVAL=300000
